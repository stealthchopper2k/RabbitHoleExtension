{
    "manifest_version": 3,
    "name": "RabbitHole Ext",
    "description": "Gathers user navigation and browsing data.",
    "version": "1.0",
    "host_permissions": [
      "*://*.google.com/"
    ],
    "permissions": ["history", "tabs", "activeTab", "storage","cookies"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  